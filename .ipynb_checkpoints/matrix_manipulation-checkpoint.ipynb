   "source": [
    "## Transposing a matrix"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Before Transpose\n",
      " [[1 2]\n",
      " [3 4]]\n",
      "After Transpose\n",
      " [[1 3]\n",
      " [2 4]]\n"
     ]
    }
   ],
   "source": [
    "matrix = np.array([[1, 2], [3, 4]])\n",
    "print(\"Before Transpose\\n\", matrix)\n",
    "transpose = matrix.T\n",
    "print(\"After Transpose\\n\",transpose)"
   ]
